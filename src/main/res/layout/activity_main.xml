<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Acessa"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context=".MainActivity">


    <ImageButton
        android:id="@+id/NetShoes"
        android:layout_width="72dp"
        android:layout_height="77dp"
        android:layout_x="285dp"
        android:layout_y="143dp"
        android:background="#FAF7F7"
        android:onClick="browserNetShoes"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/MercadoLivre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Americanas"
        app:layout_constraintVertical_bias="0.503"
        app:srcCompat="@drawable/netshoes" />

    <ImageButton
        android:id="@+id/Americanas"
        android:layout_width="72dp"
        android:layout_height="77dp"
        android:layout_marginBottom="252dp"
        android:background="#F8F6F6"
        android:onClick="browserAmericanas"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.161"
        app:srcCompat="@drawable/americanas" />

    <ImageButton
        android:id="@+id/MercadoLivre"
        android:layout_width="72dp"
        android:layout_height="77dp"
        android:background="#FAF8F8"
        android:onClick="browserMercadoLivre"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.535"
        app:srcCompat="@drawable/mercado_livre" />

    <Button
        android:id="@+id/btAluno"
        android:layout_width="112dp"
        android:layout_height="67dp"
        android:layout_marginTop="24dp"
        android:onClick="onAluno"
        android:text="@string/aluno"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.804"
        app:layout_constraintTop_toBottomOf="@+id/txtAluno" />

</androidx.constraintlayout.widget.ConstraintLayout>